<window id="detailsWindow" width="500px" sizable="true" maximizable="true" closable="true" position="center" contentStyle="overflow:auto">
    <listbox id="detailsList" rows="5" >
        <listhead>
            <listheader id="pos" label="" width="15%" sort="auto"/>
            <listheader id="detail_value" label="Value" width="45%" sort="auto"/>
            <listheader id="detail_frequency" label="Occurrence" width="20%"/>
            <listheader id="detail_ratio" label="Percentage" width="20%"/>
        </listhead>
    </listbox>
    <button id="save" label="Save" />
</window>
